@using EPiServer.Reference.Commerce.Site.Features.ProductListing.Models
@model EPiServer.Reference.Commerce.Site.Features.ProductListing.Pages.ProductListingPage

<!-- Start banner -->
@Html.DisplayFor(x => x.CarouselArea,new{HasContainer=false,HasItemContainer=false})
<!-- End banner -->
<!-- Start shop category -->
@Html.PropertyFor(x => x.ProductListing, new
{
    brand=Model.SelectedParams.Brand,
    category=Model.SelectedParams.Category,
    priceFrom=Model.SelectedParams.PriceFrom,
    priceTo=Model.SelectedParams.PriceTo,
    isSortDes=Model.SelectedParams.IsSortDes,
    pageNumber=Model.SelectedParams.PageNumber,
    HasContainer=false,
    HasItemContainer =false
})
