@using EPiServer.Reference.Commerce.Site.Features.News.Blocks
@model BlogNewsDetailBlock
@{
    Layout = null;
}

<div class="news-blog d-flex">
    <p class="news-blog_thumb">@(Model.EventDate.Value.ToString("MMMM d"))</p>
    <div>
        <h4 class="news-blog_title">@Html.PropertyFor(x => x.Title)</h4>
        <div class="news-blog_desc">@Html.PropertyFor(x => x.Description)</div>
    </div>
</div>
